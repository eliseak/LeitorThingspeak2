<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#987284" />
      <stroke
          android:width="0dp"
          android:color="#987284" />
      <corners
          android:radius="75dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <solid
          android:color="#FC8F98" />
      <stroke
          android:width="1dp"
          android:color="#FC8F98" />
      <corners
          android:radius="75dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>